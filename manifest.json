{
    "name": "wmhspgs",
    "label": "WMHS: white matter segmentation application using pgs method",
    "description": "Creates a mask of segmented white matter hyperintensities from a co-registered T1 and FLAIR image",
    "version": "0.1.0_0.5.2",
    "inputs": {
        "api_key": {
            "base": "api-key"
        },
        "FLAIR": {
            "base": "file",
            "description": "Path to directory containing FLAIR and T1 image coregistered",
            "optional": false
        },
        "T1": {
            "base":"file",
            "description":"Path to directory containing FLAIR and T1 image coregistered",
            "optional": false
        }
    },
    "config": {
    },
    "command": "python /flywheel/v0/run.py",
         "environment": {
        "FLYWHEEL": "/flywheel/v0",
        "FSLDIR": "/usr/share/fsl",
        "FSLMULTIFILEQUIT": "TRUE",
        "FSLOUTPUTTYPE": "NIFTI_GZ"
    },
    "author": "Debarun De",
    "maintainer": "Debarun De",
    "cite": "",
    "license": "BSD-3-Clause",
    "source": "",
    "url": "",
    "custom": {
        "gear-builder": {
            "category": "analysis",
            "image": "debarunde/segmentation:0.4.6"
        },
        "flywheel":{"suite": "BrainScienceCenter"}
    }

}